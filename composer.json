{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Simreka Staging Application",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.0",
        "ext-gd": "*",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "beyondcode/laravel-self-diagnosis": "^1.5",
        "cyber-duck/laravel-excel": "^2.2",
        "directorytree/ldaprecord-laravel": "^2.5",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "hashids/hashids": "^4.1",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/excel": "^3.1",
        "orangehill/iseed": "^3.0",
        "predis/predis": "^2.0",
        "rybakit/msgpack": "^0.9.1",
        "spatie/laravel-activitylog": "^3.14",
        "symfony/mailer": "^5.4",
        "symfony/var-dumper": "^5.2.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.13.0",
        "kitloong/laravel-migrations-generator": "^5.2",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Models\\": "app/Models",
            "Perks\\": "app/Perks",
            "Repository\\": "app/Repository",
            "Helper\\": "app/Helper/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper/apphelper.php",
            "app/Helper/apihelper.php",
            "app/Helper/unit_conversion_helper.php",
            "app/Helper/TenantUserHelper.php",
            "app/Helper/experimentHelper.php",
            "app/Helper/productHelper.php"
        ],
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
